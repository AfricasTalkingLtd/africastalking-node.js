    "hooks": {
      "pre-commit": "npm-run-all pre-commit-message build lint ts-check test"
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "express": "^4.17.1",
    "grpc": "^1.24.2",
    "joi": "^14.3.1",
    "query-string": "^6.11.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.9",
    "@types/chai-as-promised": "^7.1.2",
    "@types/express": "^4.17.2",
    "@types/joi": "^14.3.4",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.8.0",
